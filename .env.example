# Required
RAINDROP_ACCESS_TOKEN=your_raindrop_token_here

# Optional - Server Configuration
HTTP_PORT=3002
PORT=3002

# Optional - OAuth (if using OAuth flow)
RAINDROP_CLIENT_ID=your_client_id
